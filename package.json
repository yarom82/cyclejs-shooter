{
  "name": "cyclejs-shooter",
  "version": "1.1.0",
  "description": "Cycle.js-based hide-and-shoot game",
  "main": "src/index.js",
  "scripts": {
    "dev": "budo bootstrap.js",
    "unit": "ava",
    "coverage": "nyc npm run unit",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "test": "npm run lint && npm run coverage",
    "lint": "standard"
  },
  "keywords": [
    "shooter",
    "game",
    "cycle.js"
  ],
  "author": "Yarom Shoval <yarom.adi@gmail.com> (yarom82)",
  "contributors": [
    "Shahar Or <mightyiampresence@gmail.com> (mightyiam)"
  ],
  "license": "ISC",
  "browserify": {
    "transform": [
      "urify/transform"
    ]
  },
  "nyc": {
    "checkCoverage": true,
    "all": true,
    "include": [
      "src/**.js"
    ],
    "functions": 100,
    "branches": 100,
    "lines": 100,
    "statements": 100
  },
  "dependencies": {
    "@cycle/dom": "^15.2.0",
    "cuid": "^1.3.8",
    "ramda": "^0.23.0",
    "regalia": "^1.0.2",
    "urify": "^2.1.0",
    "xstream": "^10.0.0"
  },
  "devDependencies": {
    "@cycle/run": "^1.0.0",
    "ava": "^0.18.2",
    "budo": "^9.2.1",
    "codecov": "^1.0.1",
    "equals-regalia": "^1.0.1",
    "is-regalia": "^1.0.1",
    "is-subset": "^0.1.1",
    "mock-path-with-simple-spy": "^4.0.0",
    "mock-require": "^2.0.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.0.0",
    "require-uncached": "^1.0.3",
    "simple-spy": "^2.0.4",
    "standard": "^8.4.0",
    "stringify-object": "^3.0.0",
    "typechecker": "^4.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yarom-and-shahar/cyclejs-shooter.git"
  },
  "bugs": {
    "url": "https://github.com/yarom-and-shahar/cyclejs-shooter/issues"
  },
  "homepage": "https://github.com/yarom-and-shahar/cyclejs-shooter#readme"
}
