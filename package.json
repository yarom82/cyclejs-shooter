{
  "name": "cyclejs-shooter",
  "version": "1.0.0",
  "description": "Cycle.js-based hide-and-shoot game",
  "main": "src/index.js",
  "scripts": {
    "dev": "budo bootstrap.js",
    "unit": "ava --verbose",
    "coverage": "nyc npm run unit",
    "test": "npm run lint && npm run coverage",
    "lint": "standard"
  },
  "keywords": [
    "shooter",
    "game",
    "cycle.js"
  ],
  "author": "Yarom Shoval <yarom.adi@gmail.com> (yarom82)",
  "contributors": [
    {
      "name": "Shahar Or (mightyiam)",
      "email": "mightyiampresence@gmail.com",
      "url": "https://mightyi.am"
    }
  ],
  "license": "ISC",
  "browserify": {
    "transform": [
      "urify/transform"
    ]
  },
  "nyc": {
    "checkCoverage": true,
    "include": [
      "src/**.js"
    ],
    "functions": 100,
    "lines": 100,
    "statements": 100
  },
  "dependencies": {
    "@cycle/dom": "^13.0.0",
    "@cycle/xstream-run": "^3.1.0",
    "ramda": "^0.22.1",
    "urify": "^2.1.0",
    "xstream": "^7.0.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "budo": "^9.2.1",
    "mock-require": "^1.3.0",
    "nyc": "^8.3.2",
    "simple-spy": "^2.0.4",
    "standard": "^8.4.0",
    "stringify-object": "^2.4.0"
  }
}
